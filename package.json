{
  "name": "@tsdotnet/tween-factory",
  "version": "2.0.10",
  "description": "A strongly-typed \"tweening\" utility for use with TypeScript and JavaScript.",
  "author": "electricessence",
  "license": "MIT",
  "types": "dist/TweenFactory.d.ts",
  "main": "dist/TweenFactory.js",
  "module": "dist-esm/TweenFactory.js",
  "keywords": [
    "tween",
    "tween library",
    "tweening",
    "tween-factory",
    "tween factory",
    "animation",
    "animations",
    "animation library",
    "animate"
  ],
  "scripts": {
    "build:dist": "rimraf dist/* && tsc && copyfiles -u 1 src/**/*.d.ts dist",
    "build:dist-esm": "rimraf dist-esm/* && tsc -p tsconfig.esm.json",
    "build": "pnpm i && pnpm run build:dist && pnpm run build:dist-esm",
    "bump": "npm run-script precommit && npm version patch",
    "docs": "rimraf docs/* && typedoc --options typedoc.json --readme none",
    "lint": "eslint src/**/*.ts",
    "precommit": "pnpm i && run-p lint test && run-p build:* && npm run-script validate && git status -s",
    "prepublishOnly": "npm run-script build && run-p validate test",
    "preversion": "run-p lint test",
    "postversion": "git push && git push --tags && npm run-script docs && git add -A && git commit -m \"Updated docs.\" docs && git push",
    "test": "mocha -r ts-node/register tests/**/*.ts",
    "validate": "node ./.build/validate-package.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsdotnet/tween-factory.git"
  },
  "bugs": {
    "url": "https://github.com/tsdotnet/tween-factory/issues"
  },
  "homepage": "https://github.com/tsdotnet/tween-factory#readme",
  "devDependencies": {
    "@types/chai": "^4.3.4",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.17",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "chai": "^4.3.7",
    "copyfiles": "^2.4.1",
    "eslint": "^8.30.0",
    "eslint-config-typescript": "^3.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "rimraf": "^3.0.2",
    "ts-node": "^10.9.1",
    "typedoc": "^0.23.23",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@tsdotnet/disposable": "^1.1.24",
    "@tsdotnet/event-factory": "^1.1.8",
    "@tsdotnet/exceptions": "^1.0.19",
    "@tsdotnet/ordered-registry": "^1.0.22",
    "tslib": "^2.4.1"
  }
}
